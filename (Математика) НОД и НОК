#include <iostream>
using namespace std;

int nod(int a, int b) {
    if (a < b)
        swap(a, b);
    while (b != 0) {
        a %= b;
        swap(a, b);
    }
    return a;
}

int main() {
    int a, b;
    cin >> a >> b;
    cout << nod(a, b);
    int nok = a / nod(a, b) * b;
    cout << nok;
}
