#include <algorithm>
#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n), dp(n, 1), pr(n, -1);
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    dp[0] = 1;
    for (int i = 1; i < n; i++) 
        for (int j = 0; j < i; j++) 
            if (arr[j] < arr[i]) 
                if (dp[j] + 1 > dp[i]) {
                    dp[i] = dp[j] + 1;
                    pr[i] = j;
                }
    int maxind = 0;
    for (int i = 1; i < n; i++)
        if (dp[i] > dp[maxind])
            maxind = i;
    cout << dp[maxind] << '\n';
    vector<int> ans;
    while (maxind != -1) {
        ans.push_back(arr[maxind]);
        maxind = pr[maxind];
    }
    reverse(ans.begin(), ans.end());
    for (int i = 0; i < ans.size(); i++)
        cout << ans[i] << ' ';
}
